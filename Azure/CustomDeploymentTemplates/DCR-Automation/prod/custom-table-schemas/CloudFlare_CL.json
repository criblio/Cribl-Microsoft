{
  "description": "CloudFlare logs from CDN",
  "retentionInDays": 30,
  "totalRetentionInDays": 90,
  "columns": [
    {
      "name": "TimeGenerated",
      "type": "datetime",
      "description": "Timestamp when the record was generated"
    },
    {
      "name": "ClientIP",
      "type": "string",
      "description": "Client IP address"
    },
    {
      "name": "ClientRequestHost",
      "type": "string",
      "description": "The host requested by the client"
    },
    {
      "name": "ClientRequestMethod",
      "type": "string",
      "description": "HTTP method used in the request"
    },
    {
      "name": "ClientRequestURI",
      "type": "string",
      "description": "URI path requested"
    },
    {
      "name": "EdgeResponseStatus",
      "type": "int",
      "description": "HTTP status code returned by CloudFlare edge"
    },
    {
      "name": "EdgeResponseBytes",
      "type": "long",
      "description": "Number of bytes returned"
    },
    {
      "name": "CacheCacheStatus",
      "type": "string",
      "description": "Cache status (HIT, MISS, etc.)"
    },
    {
      "name": "EdgeServerIP",
      "type": "string",
      "description": "CloudFlare edge server IP"
    },
    {
      "name": "ClientRequestUserAgent",
      "type": "string",
      "description": "User agent string from client"
    }
  ]
}